let Point load "Point";
let BaseRectangle:3 load "../common/BaseRectangle";

fn get_random_axis(start end)
	+ start * random - end start
;

fn Rectangle(minimum maximum)
	+BaseRectangle minimum maximum Point

	#"get_random_point" fn(this)
		as(Point):2
			get_random_axis ."x"."minimum"this ."x"."maximum"this
			get_random_axis ."y"."minimum"this ."y"."maximum"this
	;
{};
